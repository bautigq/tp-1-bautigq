def main():
    a = int(input())
    b = int(input())

    suma = a + b
    diferencia = a - b
    producto = a * b
    promedio = (a + b) / 2
    cociente_entero = a // b
    resto = a % b
    division_real = a / b

    print(suma)
    print(diferencia)
    print(producto)
    print(promedio)
    print(cociente_entero)
    print(resto)
    print(division_real)

if __name__ == "__main__":
    main()
